{
  "name": "ymonitor",
  "type": "module",
  "version": "0.0.3-beta.1",
  "packageManager": "pnpm@7.1.2",
  "description": "",
  "author": "zhaoyuanda",
  "license": "MIT",
  "homepage": "https://github.com/zhaoyuanmeng/monitor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaoyuanmeng/monitor"
  },
  "bugs": {
    "url": "https://github.com/zhaoyuanmeng/monitor/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "build-tsup": "tsup src/index.ts --format cjs,esm,iife, --dts --clean",
    "build-ts": "tsup",
    "watch": "npm run build-tsup -- --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix .",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "serve": "esno server.ts",
    "server": "node server.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "tsup": "^6.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^17.0.45",
    "body-parser": "^1.20.0",
    "bumpp": "^7.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.20.0",
    "eslint-plugin-promise": "^6.0.1",
    "esno": "^0.16.3",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "pnpm": "^7.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "^0.12.10"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
